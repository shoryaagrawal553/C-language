#include <stdio.h>
 
// Function prototypes (declarations)
void hello(); // Declaring the hello function
void gb(); // Declaring the gb function
  
int main()
{
    // Function calls
    hello(); // Calling the hello function
    gb(); // Calling the gb function

    return 0; // Return statement indicating successful execution
}

// Function definitions
void hello() // Defining the hello function
{
    printf("\nHELLO!"); // Print HELLO!
}

void gb() // Defining the gb function
{
    printf("\nGood Bye!!!!!"); // Print Good Bye!!!!!
}
