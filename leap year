#include <stdio.h>

int main()
{
    int year; // Declare variable to store the year
    
    // Ask the user to enter the year
    printf("Enter the Year ");
    scanf("%d", &year);
    
    // Check if the year is a leap year
    if(year % 4 == 0)
    {
        printf("\nIt is a leap year");
    }
    else
    {
        printf("\nIt is not a leap year");
    }
}
